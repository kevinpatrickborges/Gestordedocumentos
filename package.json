{
  "name": "sgc-itep-nestjs",
  "version": "1.0.0",
  "description": "Sistema Integrado de Gest√£o Documental - ITEP-RN",
  "author": "Kevin Patrick Borges",
  "private": true,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "@": "dist/src"
  },
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "nest build",
    "build:frontend": "cd frontend && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "node scripts/check-docker.js && concurrently \"npm:start:backend\" \"npm:start:frontend\"",
    "dev:safe": "concurrently \"npm:start:backend\" \"npm:start:frontend\"",
    "start:backend": "nest start --watch",
    "start:frontend": "cd frontend && npm run dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/migration.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/migration.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/migration.config.ts",
    "seed": "ts-node src/database/seeds/run-seeds.ts",
    "check:roles": "ts-node scripts/check-roles.ts",
    "check:docker": "node scripts/check-docker.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:backup": "powershell -ExecutionPolicy Bypass -File scripts/create_dump_secure.ps1",
    "docker:restore": "powershell -ExecutionPolicy Bypass -File scripts/restore_dump_local.ps1"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.0",
    
    "@types/pg": "^8.15.5",
    "bcryptjs": "^2.4.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@playwright/test": "^1.54.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
